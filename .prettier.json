{
    "$schema": "https://json.schemastore.org/prettierrc",
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "printWidth": 100,
    "trailingComma": "all",
      "overrides": [
        {
          "files": ["slides.md", "pages/*.md"],
          "options": {
            "parser": "slidev",
            "plugins": ["prettier-plugin-slidev"]
          }
        }
      ]
  }
  